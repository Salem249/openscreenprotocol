streaming-session-sender-stats-event = {
  timestamp: microseconds
  ? audio: [1* {
    encoding-id: uint
    ? sent: microseconds
    ? encode-delay: microseconds
    ; ...
  }]
  ? video: [1* {
    encoding-id: uint
    ? encode-delay: microseconds
