; type key 142
streaming-session-receiver-stats-event = {
  timestamp: microseconds
  ? audio: [1* {
    encoding-id: uint
    ? received: microseconds
    ? lost: microseconds
    ? buffer-delay: microseconds
    ? decode-delay: microseconds
  }]
  ? video: [1* {
    encoding-id: uint
    ? received: microseconds
    ? lost: microseconds
    ? buffer-delay: microseconds
    ? decode-delay: microseconds
  }]
}

; type key 13
audio-frame = [
  encoding-id: uint
  start-time: uint
  payload: bytes
  ? optional: {
    ? 0: uint ; duration
    ? 1: media-time ; sync-time
  }
]

; type key 14
video-frame = {
  0: uint ; encoding-id
  1: uint ; sequence-number
  ? 2: [* int] ; depends-on
  3: uint ; start-time
  ? 4: uint ; duration
  5: bytes ; payload
  ? 6: uint ; rotation
  ? 7: media-time ; sync-time
}

; type key 15
data-frame = {
  0: uint; encoding-id
  ? 1: uint ; sequence-number
  ? 2: uint ; start-time
  ? 3: uint ; duration
  4: any; payload
  ? 5: media-time ; sync-time
}

; type key 143
video-frame-request = {
 1: uint; encoding-id
 2: uint; sequence-number
 ? 3: uint; highest-decoded-frame-sequence-number
}

ratio = [
  antecedent: uint
  consequent: uint
]


